{
   "swagger": "2.0",
   "info": {
     "title": "Swagger",
     "description": "This is a sample server",
     "version": "1.0"
   },
   "produces": ["application/json"],
   "host": "localhost:8081",
   "basePath": "/",
   "$schema": ["http", "https"],
   "basedir": "__direname",
   "files": ["pmm-server/src/resource/travelResource.js", "pmm-server/src/resource/partyResource.js"],
   "components": {
    "schemas": {
       "Driver": {
         "type": "object",
         "properties": {
           "firstName": {
             "type": "string"
           },
           "lastName": {
             "type": "string"
           },
           "lisence": {
             "type": "string"
           }
         }
       }
    }
  },
  "paths": {
    "/travels": {
      "post": {
        "x-swagger-router-controller": "travelResource",
        "description": "Return a driver for you travel",
        "requestBody": {
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "propeties": {
                "latitude": {
                  "type": "string"
                },
                "longitude": {
                  "type": "string"
                }
              }
            }
          }
        }
        }
      }
    },
    "/drivers": {
      "get": {
        "x-swagger-router-controller": "partyResource",
        "description": "Return all drivers",
        "parameters": [],
        "responses": {
        "200": {          
          "description": "A list of drivers.",
          "content": {
            "application/json": {
              "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Driver"
                  }                  
              }
            }
          }
        }
      }
      }
    }
  }
}